{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "dist",
        "jsx": "react",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "strict": true,
        "noEmit": true,
        "allowJs": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "esModuleInterop": true
    },
    "include": [
        "jestSetup.ts",
        "src"
    ]
}
